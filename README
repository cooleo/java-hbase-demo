create 'liendien',{NAME=>'family1'}

scan 'btestfeed', { COLUMNS => 'info:title', LIMIT => 10, FILTER => "ValueFilter( =, 'binaryprefix:hello feed' )" }
scan 'btestfeed', { COLUMNS => 'category', LIMIT => 10, FILTER => "ValueFilter( =, 'binaryprefix:Tinh Cam' )" }